##################################################################
#Color Rules
##################################################################

# Red Off
rule 100 "Red Off" {
 =0 : "red-1"
 >=1 : "green-1"
 default : "green-1"
}
 
# Green Off
rule 101 "Green Off" {
 =0 : "green-1"
 >=1 : "red-1"
 default : "red-1"
} 

# RF Main
rule 102 "RF Main" {
 >=0 && <1 : "white"
 >=1 && <1.1 : "red-1"
 >=1.1 && <1.2 : "pink"
 >=1.2 && <1.3 : "cyan-3"
 >=2 && <4 : "gray-8"
 >=4 && <6 : "black"
 >=6 && <60 : "cyan-5"
}
# RF On
rule 103 "RF On" {
 >=0 && <2 : "red-1"
 >=2 && <3 : "cyan-5"
 >=3 && <5 : "red-1"
 >=5 && <6 : "gray-8"
 >=6 && <60 : "cyan-5"
 }
# RF Tuner
rule 104 "RF Tuner" {
 >=0 && <2 : "red-1"
 >=2 && <4 : "gray-8"
 >=4 && <6 : "black"
 >=6 && <15 : "cyan-5"
 >=15 && <24 : "yellow-1"
 >=24 && <33 : "gray-8"
 >=33 && <42 : "cyan-5"
 >=42 && <51 : "yellow-1"
 >=51 && <60 : "gray-8"
}
# RF SOS
rule 105 "RF SOS" {
 >=0 && <1 : "white"
 >=1 && <1.1 : "red-1"
 >=1.1 && <1.2 : "pink"
 >=1.2 && <1.3 : "cyan-3"
 >=2 && <4 : "gray-8"
 >=4 && <6 : "black"
 >=6 && <33 : "cyan-5"
 >=33 && <60 : "white" 
}

# RF GLDER
rule 106 "RF GLDER" {
 >=0 && <2 : "red-1"
 >=2 && <4 : "gray-8"
 >=4 && <6 : "black"
 >=6 && <7 : "cyan-5"
 >=7 && <8 : "red-1"
 >=8 && <9 : "yellow-1"
 >=9 && <10 : "red-1"
 >=10 && <12 : "cyan-5"
 >=12 && <13 : "yellow-1"
 >=13 && <14 : "red-1"
 >=14 && <15 : "yellow-1"
 >=15 && <16 : "red-1"  
}

# RF PLDER
rule 107 "RF PLDER" {
 >=0 && <2 : "red-1"
 >=2 && <4 : "gray-8"
 >=4 && <6 : "black"
 >=6 && <10 : "cyan-5"
 >=10 && <11 : "yellow-1"
 >=11 && <12 : "red-1"
 >=12 && <13 : "yellow-1"
 >=13 && <15 : "red-1"
 >=15 && <16 : "yellow-1" 
}

# Viewer
rule 108 "Viewer" {
 >=0 && <1 : "gray-8"
 >=1 && <2 : "green-1"
 >=2 && <3 : "yellow-1"
 >=3 && <4 : "red-1"
}
 
# Loop
rule 109 "Loop" {
 >=0 && <1 : "green-1"
 >=1 && <2 : "gray-8"
}

# Energy_Lock
rule 110 "Energy_Lock" {
 >=0 && <8 : "green-1"
 >=8 && <9 : "yellow-1"
 >=9 && <10 : "red-1"
}

# RF State
rule 111 "RF_State" {
 >=0 && <2 : "terracotta"
 >=2 && <4 : "gray-9"
 >=4 && <6 : "gray-12"
 >=6 && <33 : "gray-3"
 >=33 && <60 : "red-5"
}

# Ramp
rule 112 "Ramp" {
 =0 : "yellow-1"
 >=1 : "green-1"
}

# Alh
rule 113 "Alh" {
 <0 : "gray-9"
 =0 : "green-1"
 >=1 && <2 : "yellow-1"
 >=2 && <3 : "red-1"
 >=4 : "white"
}

# LockOrbit
rule 114 "LockOrbit" {
 >=0 && <1 : "gray-4"
 >=1 && <2 : "yellow-3"
}

# LockEnergy
rule 115 "LockEnergy" {
 >=0 && <1 : "gray-4"
 >=1 && <2 : "blue-1"
}

# Bpm On
rule 116 "Bpm On" {
 >=0 && <1 : "red-1"
 >=1 && <2 : "gray-8"
}

# Bpm Off
rule 117 "Bpm Off" {
 >=0 && <1 : "gray-8"
 >=1 && <2 : "red-1"
}

# RF Gradient
rule 118 "Gradient" {
 >=0 && <2 : "black"
 >=2 && <3 : "lilac-5"
 >=3 && <4 : "lilac-3"
 >=4 && <5 : "lilac-1"
 >=5 && <6 : "cyan-5"
 >=6 && <7 : "cyan-4"
 >=7 && <8 : "cyan-3"
 >=8 && <9 : "cyan-1"
 >=9 && <10 : "laural"
 >=10 && <11 : "forest"
 >=11 && <12 : "atlantis"
 >=12 && <13 : "orange-2"
 >=13 && <14 : "yellow-3"
 >=14 && <15 : "yellow-1"
 
}

# BOOM
rule 119 "BOOM" {
 >=0 && <1 : "tan-1"
 >=1 && <2 : "red-2"
 >=2 && <3 : "yellow-1"
 >=3 && <4 : "yellow-3"
 >=4 && <5 : "green-1"
}

# PSS
rule 120 "PSS" {
 >=0 && <1 : "white"
 >=1 && <2 : "green-3"
 >=2 && <3 : "yellow-1"
 >=3 && <4 : "yellow-1"
 >=4 && <5 : "yellow-3"
 >=5 && <6 : "red-1"
 >=6 && <7 : "purple-4"
 >=7 && <8 : "purple-4"
}

# BLM_lights
rule 121 "BLM_light"{
 >=0 && <15 : "black"
 >=15 && <26 : "green-5"
 >=26 && <37 : "green-4"
 >=37 && <48 : "green-3"
 >=48 && <59 : "green-2"
 >=59 && <70 : "green-1"
 >=70 && <79 : "yellow-1"
 >=79 && <88 : "yellow-2"
 >=88 && <97 : "yellow-3"
 >=97 && <106 : "orange-1"
 >=106 && <115 : "orange-2"
 >=115 && <120 :"red-3"
 >=120 : "red-1"
 }

# mA_1
rule 122 "mA_1" {
 >=0 && <13 : "cyan-2"
 >=13 && <1000000 : "pink"
}

# mA_2
rule 123 "mA_2" {
 >=0 && <13 : "cyan-4"
 >=13 && <1000000 : "purple-2"
} 

#BPM_Loss
rule 124 "BPM_Loss" {
 >=0 && < .1 : "gray-12"
 >=.1 && <  .5 : "yellow-2"
 >=.5 && < 1.0 : "yellow-3"
 >=1.00 && < 2.0 : "orange-1"
 >=2.0 && < 5.0 : "orange-2"
 > 5.0 : "red-1"
 default : "gray-12"
}

# Blue Off
rule 134 "Blue Off" {
 =0 : "cyan-3"
 >=1 : "green-1"
 default : "cyan-3"
}

# Blue On
rule 135 "Blue On" {
 =0 : "green-1"
 >=1 : "cyan-3"
 default : "cyan-3"
 }

# Gray Off
rule 136 "Gray Off" {
 =0 : "gray-8"
 >=1 : "green-1"
 default : "white"
}

# Gray On
rule 137 "Gray On" {
 =0 : "green-1"
 >=1 : "gray-8"
 default : "white"
}
 
#Invisible Red
rule 138 "Invisible Red" {
 =0 : "invisible"
 >=1 : "red-1"
 default : "white"
}

#Invisible ReverseRed
rule 139 "Invisible ReverseRed" {
 =0 : "red-1"
 >=1 : "invisible"
 default : "white"
}

#Invisible Green
rule 140 "Invisible Green" {
 =0 : "invisible"
 >=1 : "green-1"
 default : "white"
 }

#Invisible ReverseGreen
rule 141 "Invisible ReverseGreen" {
 =0 : "green-1"
 >=1 : "invisible"
 default : "white"
}

#Invisible Yellow
rule 142 "Invisible Yellow" {
 =0 : "invisible"
 >=1 : "yellow-1"
 default : "white"
}

#Invisible ReverseYellow
rule 143 "Invisible ReverseYellow" {
 =0 : "yellow-1"
 >=1 : "invisible"
 default : "white"
}

#Invisible Black
rule 144 "Invisible Black" {
 =0 : "invisible"
 >=1 : "black"
 default : "white"
}

#Invisible Reverseblack
rule 145 "Invisible Reverseblack" {
 =0 : "black"
 >=1 : "invisible"
 default : "white"
}

#Warning
rule 146 "Warning" {
  =0 : "black"
  >=1 : "yellow-1"
 default : "white"
}

#TagLoop
rule 147 "TagLoop" {
 >=0 && <1 : "gray-8"
 >=1 && <3 : "yellow-1"
 >=3 && <4 : "green-1"
}

